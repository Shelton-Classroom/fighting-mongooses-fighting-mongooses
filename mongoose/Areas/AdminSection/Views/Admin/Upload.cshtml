
@{
    ViewBag.Title = "Upload";
    var message = ViewBag.Success;
}
@Html.Partial("_adminNav")
<h2>Upload</h2>

@using (Html.BeginForm("Upload", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <label class="custom-upload" for="file">Upload Student Course Data</label>
    <input type="file" name="UploadedFile" accept=".xlsx" style="width: 100%;" />
    <br />
    <input type="submit" name="Submit" value="Submit" class="submit btn btn-default" />
}
<br />
@using (Html.BeginForm("UploadMajor", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <label class="custom-upload" for="file">Upload Student Major Data</label>
    <input type="file" name="UploadedFile" accept=".xlsx" style="width: 100%;" />
    <br />
    <input type="submit" name="Submit" value="Submit" class="submit btn btn-default" />
}

<script type="text/javascript">
    var message = '@message';
    if(message){
        alert(message);
    }
    console.log(message);
</script>